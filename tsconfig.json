{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "#imports": [".nuxt/types/imports"]
    }
  },
  "include": [
    ".nuxt",
    "nuxt.config.ts",
    "server",
    "server/api",
    "server/plugins",
    "server/utils",
    "server/models",
    "middleware",
    "scripts",
    "pages",
    "components",
    "composables",
    "plugins",
    "types"
  ],
  "types": ["@nuxt/types"]
}
