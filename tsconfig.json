{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "#imports": [".nuxt/types/imports"]
    },
    "moduleResolution": "Bundler",
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "types": ["node", "nuxt"],
    "skipLibCheck": true
  },
  "include": [
    ".nuxt",
    "nuxt.config.ts",
    "server",
    "server/api",
    "server/plugins",
    "server/utils",
    "server/models",
    "middleware",
    "scripts",
    "pages",
    "components",
    "composables",
    "plugins",
    "types"
  ],
}
