{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ]
    }
  },
  "include": [
    ".nuxt",
    "nuxt.config.ts",
    "server",
    "server/api",
    "server/plugins",
    "server/utils",
    "server/models",
    "middleware",
    "scripts",
    "pages",
    "components",
    "types"
  ]
}
